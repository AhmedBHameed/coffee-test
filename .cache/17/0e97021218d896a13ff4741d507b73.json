umn":9}},{"name":"gameActions","generated":{"line":172,"column":81},"source":"components/game-wapper.jsx","original":{"line":101,"column":14}},{"generated":{"line":172,"column":92},"source":"components/game-wapper.jsx","original":{"line":101,"column":9}},{"generated":{"line":172,"column":94},"source":"components/game-wapper.jsx","original":{"line":99,"column":6}},{"generated":{"line":172,"column":95},"source":"components/game-wapper.jsx","original":{"line":98,"column":4}},{"generated":{"line":173,"column":0},"source":"components/game-wapper.jsx","original":{"line":104,"column":3}},{"name":"React","generated":{"line":177,"column":2},"source":"components/game-wapper.jsx","original":{"line":35,"column":41}},{"name":"Component","generated":{"line":177,"column":17},"source":"components/game-wapper.jsx","original":{"line":35,"column":47}}],"sources":{"components/game-wapper.jsx":"import React from \"react\";\nimport styled from \"styled-components\";\n\n// Component\nimport Cup from \"./cup.jsx\";\nimport FlexWrapper from \"./flex-wrapper\";\n\nconst CupFlex = styled(FlexWrapper)`\n  align-items: center;\n  flex-grow: 1;\n  justify-content: center;\n  flex-wrap: wrap;\n  margin: 10px 0;\n`;\n\nconst GameOver = styled.h2`\n  color: red;\n`;\n\nconst GamePaused = styled.h2`\n  color: brown;\n`;\n\nconst ColumnFlex = styled.div`\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n`;\n\nconst Content = styled.div`\n  display: flex;\n  flex-direction: column;\n`;\n\nexport default class GameWrapper extends React.Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      gamePause: false\n    };\n\n    this.pauseOrPlay = this.pauseOrPlay.bind(this);\n  }\n\n  pauseOrPlay() {\n    this.setState({\n      gamePause: !this.state.gamePause\n    });\n  }\n\n  gameActions() {\n    const { gameover, setSettings, handleStart } = this.props;\n    const { gamePause } = this.state;\n    return (\n      <React.Fragment>\n        <ColumnFlex className=\"columnFlex\">\n          {gamePause && <GamePaused>GAME PAUSED</GamePaused>}\n          {gameover && (\n            <React.Fragment>\n              <GameOver>GAME OVER</GameOver>\n              <button className=\"actionButton\" onClick={handleStart}>\n                Retry\n              </button>\n              <button className=\"actionButton\" onClick={() => setSettings()}>\n                Change settings\n              </button>\n            </React.Fragment>\n          )}\n        </ColumnFlex>\n        {!gameover && (\n          <button className=\"actionButton\" onClick={this.pauseOrPlay}>\n            {gamePause ? \"Play\" : \"Pause\"}\n          </button>\n        )}\n      </React.Fragment>\n    );\n  }\n\n  renderCups() {\n    const { setGameOver, defaultTimeout, nbCups, gameover } = this.props;\n    const { gamePause } = this.state;\n    let cups = [];\n    for (var i = 0; i < nbCups; i++) {\n      cups.push(\n        <Cup\n          key={i.toString()}\n          isGameover={gameover}\n          gamePause={gamePause}\n          handleGameOver={setGameOver}\n          defaultTimeout={defaultTimeout}\n        />\n      );\n    }\n    return <CupFlex>{cups}</CupFlex>;\n  }\n\n  render() {\n    return (\n      <Content>\n        {this.renderCups()}\n        {this.gameActions()}\n      </Content>\n    );\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"10aed0a18321a8c96d10b4a602996d70","cacheData":{"env":{}}}